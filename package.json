{
  "name": "ht-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.1.2",
    "@loadable/component": "^5.15.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "bootstrap-icons": "^1.5.0",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "uuid": "^8.3.2",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false craco build",
    "postbuild": "react-snap && purgecss --css build/static/css/*.css --content build/*.html build/static/js/*.js --output build/static/css",
    "prehost": "npm run build",
    "host": "gh-pages -m 'auto committed updates' -b web -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "gh-pages": "^3.2.3",
    "react-snap": "^1.23.0",
    "sass": "^1.35.1"
  },
  "reactSnap": {
    "inlineCss": true
  },
  "homepage": "https://www.heinthanth.com"
}
